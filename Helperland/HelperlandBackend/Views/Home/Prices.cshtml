@{
    ViewData["Title"] = "Price and Services";
    Layout = "~/Views/Shared/_Layout1.cshtml";
}





<body>
    @Html.Partial("_Login")
    <link href="~/css/Prices.css" rel="stylesheet" />



    <img src="~/assets/img/group-18.png" class="responsive_img" alt="" style="width:100%">

    <div class="container div1">
        <p class="faq1">Prices</p>
        <div class="imgcont">
            <div class="line">
                <figure><img src="~/assets/img/star.png" alt=""></figure>
            </div>
            <br>
        </div>
        <div class="div3">
            <div class="div2">
                <h2>One Time</h2>
            </div>

            <h2 class="head2">€ 20<span class="head3">/hr</span></h2>

            <div class="div5">
                <div class="div4">
                    <img src="~/assets/img/forma-1_5.png" alt="">
                    <p>Lower prices</p>
                </div>
                <div class="div4">
                    <img src="~/assets/img/forma-1_5.png" alt="">
